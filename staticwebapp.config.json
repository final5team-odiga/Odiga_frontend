{
  "platform": {
    "apiRuntime": "node:20"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/.auth/me",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/auth/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/articles*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/profile*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/magazine*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/storage*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/speech*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  },
  "networking": {
    "allowedIpRanges": [],
    "enablePrivateAccess": false
  },
  "apiUrl": "https://odigawepapp.azurewebsites.net"
}
